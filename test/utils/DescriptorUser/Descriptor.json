				}
			}
		},
		"schemas": {
			"Status": {
				"type": "string",
				"enum": [ "ENABLED", "INITIALIZED", "DISABLED" ]
			},
			"TestInteger": {
				"type": "integer",
				"maximum": 5
			},
			"TestNumber": {
				"type": "number",
				"maximum": 0.5
			},
			"TestString": {
				"type": "string",
				"minLength": 1,
				"maxLength": 5
			},
			"TestObject": {
				"type": "object",
				"properties": {
					"body-param-string": {
						"$ref": "#/components/schemas/TestString"
					}
				},
				"required": [ "body-param-string" ]
			},
			"Error": {
				"type": "object",
				"required": [
					"code",
					"message"
				],
				"properties": {
					"code": {
						"type": "string"
					},
					"message": {
						"type": "string"
					}
				}
			}
