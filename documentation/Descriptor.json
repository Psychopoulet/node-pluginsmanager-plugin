	"components": {
		"parameters": {
			"UrlParam": {
				"name": "url-param",
				"in": "query",
				"description": "Entity name",
				"required": true,
				"schema": {
					"$ref": "#/components/schemas/SearchParameter"
				}
			}
		},
		"schemas": {
			"SearchParameter": {
				"type": "string",
				"minLength": 3,
				"maxLength": 50
			},
			"SearchResult": {
				"type": "object",
				"required": [
					"search",
					"content"
				],
				"properties": {
					"search": {
						"type": "string"
					},
					"content": {
						"type": "string"
					}
				}
			},
			"Error": {
				"type": "object",
				"required": [
					"code",
					"message"
				],
				"properties": {
					"code": {
						"type": "string"
					},
					"message": {
						"type": "string"
					}
				}
			}
		}
